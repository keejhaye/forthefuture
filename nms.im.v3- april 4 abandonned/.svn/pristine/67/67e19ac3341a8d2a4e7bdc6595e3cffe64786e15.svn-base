/* Sticky footer styles
-------------------------------------------------- */
html {
  position: relative;
  min-height: 100%;
}
.footer {
  position: fixed;
  bottom: 0;
  width: 100%;
  /* Set the fixed height of the footer here */
  height: 25px;
  background-color: #f5f5f5;
}


/* Custom page CSS
-------------------------------------------------- */
/* Not required for template or sticky footer method. */

.footer-container-fluid {
  width: auto;
  /* max-width: 750px; */
  padding: 0 0px;
}
.footer-container-fluid .text-muted {
  margin: 5px 0;
  text-align: center;
  width: 100%;
}